

{% extends 'a_base.html' %}
{% load static %}

{% block content %} 
<center>
<h2>Skills Dataset Upload
<hr>
</h2>
<h5><font size="" color="#666699">{{msg}}</font></h4>
<form method="post" action="/skillsdataset/">
	{% csrf_token %} 
	<table id="tab" width="60%">
	<tr>
		<td><h4>Upload Dataset</h4>
		<tr>	<td>
		
		<input type="file" name="file" required accept=".csv" class="form-control">
		
		<tr><td>

		<input type="submit" value="Upload" class="form-control"></td>
	</tr>
	</table>
</form>


<hr>
<br>







    {% if data %}


<table id="tab" width="100%">
				<tr><th>#<th>Role<th>Skill Set<th>Soft Skills<th>Advanced Concepts<th>Certifications

        {% for d1 in data %}
        
					<tr><td>{{ forloop.counter }}<td><h5 class="card-title fw-bold">{{ d1.Role }}</h5>
					<td>{{d1.Skills}}
					<td>{{d1.SoftSkills}}
										<td>{{d1.AdvancedConcepts}}
															<td>{{d1.Certifications}}
					
        {% endfor %}
    </table>
    
	{% else %}
    <h4>No Dataset Available</h4>

	
	{% endif %}







{% endblock %}